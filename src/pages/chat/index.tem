<Wrapper className="{{className}}">
  <Sidebar>
    <Sidebar.Header>
      <Link to="/home">
        <Button href="/" variant="icon" className="home-link" title="&#8962;" />
      </Link>
      <User.ProfileLink />
      <Chat.SearchForm onSubmit={{searchChat}} />
    </Sidebar.Header>
    <Sidebar.Body>
      <Chat.List chats={{chats}} onClick={{ChatItemSelected}} />
    </Sidebar.Body>
  </Sidebar>
  <Main>
    <Chat.Header />
    <Chat.Messages />
    <Chat.Footer />
  </Main>
</Wrapper>
