<Wrapper className="login-form">
  <Form>
    <Form.Header>
      Регистрация
    </Form.Header>
    <Form.Body onBlur={{validate}}>
      {{inputsView}}
      {{this.formError ? <h3 class="form-error login-form_form-error">{{formError}}</h3> : null}}
    </Form.Body>
    <Form.Footer>
      {{buttons}}
    </Form.Footer>
  </Form>
</Wrapper>
